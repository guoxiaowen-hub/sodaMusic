/* 空白 */
.blank{
  height: 10px;
}

/* 搜索框 */
.search{
  width: 90%;
  height:30px;
  margin: auto auto;
  border:solid 0px #000000;   /**/
  border-radius:20px;
  font-size: 12px;
  background-color: #76C2AC;
  position: relative;
}
/* 
.search > .search_view{
  width: auto;
